{
    "name": "webup/laravel-sendinblue",
    "description": "Laravel's mail transport for SendinBlue",
    "keywords": [
        "laravel",
        "laravel5",
        "laravel7",
        "laravel8",
        "lumen",
        "sendinblue",
        "stmp",
        "mailing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Ducatel",
            "email": "baptiste@agence-webup.com",
            "homepage": "http://agence-webup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "illuminate/mail": "^7.0|^8.0|^9.0|^10.0",
        "sendinblue/api-v3-sdk": "^7.4.4|^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Webup\\LaravelSendinBlue\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webup\\LaravelSendinBlue\\SendinBlueServiceProvider"
            ]
        }
    }
}
